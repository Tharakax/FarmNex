{
    "configurations": [
        {
            "name": "Arduino",
            "includePath": [
                "${workspaceFolder}/**",
                "${env:LOCALAPPDATA}/Arduino15/packages/arduino/hardware/avr/1.8.6/cores/arduino",
                "${env:LOCALAPPDATA}/Arduino15/packages/arduino/hardware/avr/1.8.6/libraries/**",
                "${env:LOCALAPPDATA}/Arduino15/packages/arduino/hardware/avr/1.8.6/variants/standard",
                "C:/Users/Win10/Documents/Arduino/libraries/**",
                "${env:LOCALAPPDATA}/Arduino15/packages/**"
            ],
            "defines": [
                "ARDUINO=10819",
                "ARDUINO_AVR_UNO",
                "ARDUINO_ARCH_AVR",
                "__AVR_ATmega328P__",
                "F_CPU=16000000L",
                "USB_VID=0x2341",
                "USB_PID=0x0043",
                "USB_MANUFACTURER=\"Arduino LLC\"",
                "USB_PRODUCT=\"Arduino Uno\"",
                "ARDUINO_MAIN=1"
            ],
            "compilerPath": "${env:LOCALAPPDATA}/Arduino15/packages/arduino/tools/avr-gcc/7.3.0-atmel3.6.1-arduino7/bin/avr-gcc.exe",
            "cStandard": "c11",
            "cppStandard": "c++11",
            "intelliSenseMode": "gcc-x86",
            "forcedInclude": [
                "${env:LOCALAPPDATA}/Arduino15/packages/arduino/hardware/avr/1.8.6/cores/arduino/Arduino.h",
                "${workspaceFolder}/arduino_stub.h"
            ]
        }
    ],
    "version": 4
}